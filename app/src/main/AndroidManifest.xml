        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OhMyGodCart"
        tools:targetApi="31">
        <activity
            android:name=".activites.PaymentActivity"
            android:exported="false" />
        <activity
            android:name=".activites.AddAddressActivity"
            android:exported="false" />
        <activity
            android:name=".activites.AddressActivity"
            android:exported="false" />
        <activity
            android:name=".activites.CartActivity"
            android:exported="false" />
        <activity
            android:name=".activites.ShowAllActivity"
            android:exported="false" />
        <activity
            android:name=".activites.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activites.Onboarding"
            android:exported="false" />
        <activity
            android:name=".activites.Sign_up"
            android:exported="false" />
        <activity
            android:name=".activites.Sign_in"
            android:exported="false" />
        <activity
            android:name=".activites.MainActivity"
            android:exported="true" />
        <activity
            android:name=".activites.Reset_password"
            android:exported="false" />
        <activity
            android:name=".activites.Verification"
            android:exported="false" />
        <activity
            android:name=".activites.Password_recovery"
            android:exported="false" />
        <activity
            android:name=".activites.DetailedActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <meta-data
            android:name="com.razorpay.Apikey"
            android:value="rzp_live_yoggonx4XZrTXB" />
    </application>

</manifest>